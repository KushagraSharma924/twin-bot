# Render Ollama Configuration
OLLAMA_HOST=https://yourdomain.onrender.com/ollama
OLLAMA_MODEL=llama3.2
OLLAMA_CONNECT_TIMEOUT=10000  # Increased timeout for production environment

# Set NODE_ENV for production
NODE_ENV=production

# TensorFlow Learning Configuration
EMBEDDING_DIM=384
LEARNING_RATE=0.001
MODELS_DIR=./models/ollama-tf
ENABLE_TENSORFLOW_LEARNING=true

# Server Configuration
PORT=10000  # Render may assign a different port - will be set by Render's PORT environment variable 