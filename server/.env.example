# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Azure OpenAI Configuration
AZURE_OPENAI_KEY=your-azure-openai-api-key
AZURE_OPENAI_ENDPOINT=your-azure-openai-endpoint

# Google API Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:5002/api/auth/google/callback

# Email OAuth Credentials
# For Gmail
GOOGLE_EMAIL_CLIENT_ID=your_google_email_client_id
GOOGLE_EMAIL_CLIENT_SECRET=your_google_email_client_secret

# For Outlook/Microsoft
MICROSOFT_EMAIL_CLIENT_ID=your_microsoft_client_id
MICROSOFT_EMAIL_CLIENT_SECRET=your_microsoft_client_secret

# For Yahoo Mail
YAHOO_CLIENT_ID=your_yahoo_client_id
YAHOO_CLIENT_SECRET=your_yahoo_client_secret

# Server Configuration
PORT=5002
NODE_ENV=development
SERVER_URL=http://localhost:5002
SITE_URL=http://localhost:3000

# Security
JWT_SECRET=your_jwt_secret
ENCRYPTION_KEY=your-encryption-key-for-sensitive-data
JWT_EXPIRES_IN=7d

# Feature Flags
ENABLE_BROWSER_INSIGHTS=true
ENABLE_REINFORCEMENT_LEARNING=true
ENABLE_CALENDAR_INTEGRATION=true

# AI Configuration
OPENAI_API_KEY=your_openai_api_key
GOOGLE_GEMINI_API_KEY=your_gemini_api_key 